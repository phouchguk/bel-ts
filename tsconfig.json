{
  "compilerOptions": {
    "strict": true,
    "strictFunctionTypes": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "target": "es6",
    "module": "commonjs",
    "newLine": "LF",
    "outDir": "js"
  },
  "files": ["./main.ts"]
}
